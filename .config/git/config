[core]
editor = code --wait

[alias]
can = commit --amend --no-edit
fetchall = fetch --all --prune --tags
pro = pull --rebase origin
pushf = push --force-with-lease

[branch]
autosetuprebase = always

[commit]
gpgsign = true

[credential]
helper = osxkeychain

[filter "lfs"]
clean = git-lfs clean -- %f
smudge = git-lfs smudge -- %f
process = git-lfs filter-process
required = true

