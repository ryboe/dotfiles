{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[go.mod]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.formatOnSave": true
    },
    "[go]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.formatOnSave": true,
        "editor.snippetSuggestions": "none"
    },
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "breadcrumbs.enabled": true,
    "debug.allowBreakpointsEverywhere": true,
    "debug.inlineValues": true,
    "diffEditor.renderSideBySide": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "gutter",
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        100
    ],
    "editor.tabCompletion": "on",
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "*.hcl": "terraform",
        "Cargo.lock": "toml"
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.blame.ignoreWhitespace": true,
    "go.autocompleteUnimportedPackages": true,
    "go.formatFlags": [
        "-s"
    ],
    "go.formatTool": "goimports",
    "go.lintTool": "golangci-lint",
    "go.useLanguageServer": true,
    "gopls": {
        "staticcheck": false,
        "usePlaceholders": true
    },
    "html.format.endWithNewline": true,
    "markdownlint.ignore": [
        "MD026"
    ],
    "pgsql.format.alignColumnDefinitionsInColumns": true,
    "pgsql.format.datatypeCasing": "lowercase",
    "pgsql.format.keywordCasing": "lowercase",
    "pgsql.format.placeSelectStatementReferencesOnNewLine": true,
    "pgsql.intelliSense.lowerCaseSuggestions": true,
    "python.analysis.completeFunctionParens": true,
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.terminal.activateEnvironment": false,
    "rust-client.autoStartRls": false,
    "rust-client.engine": "rust-analyzer",
    "rust-client.updateOnStartup": true,
    "rust.clippy_preference": "on",
    "rust.wait_to_build": 1000,
    "search.showLineNumbers": true,
    "shellformat.flag": "-s",
    "tabnine.experimentalAutoImports": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.rendererType": "experimentalWebgl",
    "vs-kubernetes": {
        "vs-kubernetes.draft-path": "/Users/ryan/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.helm-path": "/Users/ryan/.vs-kubernetes/tools/helm/darwin-amd64/helm"
    },
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Monokai",
    "workbench.commandPalette.history": 1000,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none"
}
